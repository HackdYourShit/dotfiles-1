"*":
  core:
    disabledPackages: [
      "exception-reporting"
    ]
    themes: [
      "dracula-ui"
      "dracula-theme"
    ]
  editor:
    invisibles: {}
  "exception-reporting": {}
  "linter-cppcheck": {}
  "linter-cpplint":
    extensions: "cpp,h,ino"
    filters: "-legal"
  "linter-gcc": {}
  "linter-ruby": {}
  "linter-shellcheck":
    enableNotice: true
  "markdown-preview":
    useGitHubStyle: true
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  "vim-mode-plus":
    startInInsertMode: true
  welcome:
    showOnStartup: false
